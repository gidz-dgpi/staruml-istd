	<xs:simpleType name="LDT_BeperkingScore">
		<xs:annotation>
			<xs:documentation>Gecodeerde aanduiding van de score op een vraag over een beperking.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:annotation>
				<xs:appinfo>
					<iwlz:codelijstNaam>COD828: Score beperking</iwlz:codelijstNaam>
				</xs:appinfo>
			</xs:annotation>
			<xs:enumeration value="0">
				<xs:annotation>
					<xs:documentation>Kan deze activiteit zelf, er is geen hulp van een ander nodig</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="1">
				<xs:annotation>
					<xs:documentation>Kan het zelf, maar iemand anders moet toezien of stimuleren</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="2">
				<xs:annotation>
					<xs:documentation>Kan dit slechts met veel moeite/iemand anders moet helpen</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="3">
				<xs:annotation>
					<xs:documentation>Kan dit niet zelfstandig/iemand anders moet dit overnemen</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="9">
				<xs:annotation>
					<xs:documentation>Niet beoordeeld</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:maxLength value="1"/>
		</xs:restriction>
	</xs:simpleType>
